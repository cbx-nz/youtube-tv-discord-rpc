{
  "manifest_version": 3,
  "name": "YouTube TV Discord RPC",
  "version": "1.0",
  "permissions": ["scripting"],
  "host_permissions": ["https://www.youtube.com/tv/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/tv*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}